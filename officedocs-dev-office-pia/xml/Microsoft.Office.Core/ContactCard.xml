        <ReturnType>System.Void</ReturnType>
      </ReturnValue>
      <Parameters>
        <Parameter Name="CardStyle" Type="Microsoft.Office.Core.MsoContactCardStyle" />
        <Parameter Name="RectangleLeft" Type="System.Int32" />
        <Parameter Name="RectangleRight" Type="System.Int32" />
        <Parameter Name="RectangleTop" Type="System.Int32" />
        <Parameter Name="RectangleBottom" Type="System.Int32" />
        <Parameter Name="HorizontalPosition" Type="System.Int32" />
        <Parameter Name="ShowWithDelay" Type="System.Boolean" />
      </Parameters>
      <Docs>
        <param name="CardStyle">
          <see cref="T:Microsoft.Office.Core.MsoContactCardStyle" />它确定是否卡片显示为悬浮卡片或为完全展开卡片。</param>
        <param name="RectangleLeft">指定不显示卡片的矩形左侧的 x 坐标。</param>
        <param name="RectangleRight">指定不显示卡片的矩形右侧的 x 坐标。</param>
        <param name="RectangleTop">指定不显示卡片的矩形顶部的 y 坐标。</param>
        <param name="RectangleBottom">指定不显示卡片的矩形底部的 y 坐标。</param>
        <param name="HorizontalPosition">指定卡片左边缘的 x 坐标位置。</param>
        <param name="ShowWithDelay">确定在卡片显示之前是否有延迟。</param>
        <summary>在指定矩形外的指定 x 坐标处显示联系人卡片。</summary>
        <remarks>
          <para>
            <paramref name="Style" />参数必须为之一<see cref="T:Microsoft.Office.Core.MsoContactCardStyle" />下表中的值。</para>
          <list type="table">
            <item>
              <description>
                <b>msoContactCardTypeEnterpriseContact</b>
              </description>
              <description>0</description>
              <description>代表企业联系人地址的联系人卡片。</description>
            </item>
            <item>
              <description>
                <b>msoContactCardTypePersonalContact</b>
              </description>
              <description>1</description>
              <description>代表个人联系人地址的联系人卡片。</description>
            </item>
            <item>
              <description>
                <b>msoContactCardTypeUnknownContact</b>
              </description>
              <description>2</description>
              <description>表示未知联系人地址的联系人卡片。</description>
            </item>
            <item>
              <description>
                <b>msoContactCardTypeEnterpriseGroup</b>
              </description>
              <description>3</description>
              <description>表示企业通讯组列表联系人地址的联系人卡片。</description>
            </item>
            <item>
              <description>
                <b>msoContactCardTypePersonalDistributionList</b>
              </description>
              <description>4</description>
              <description>表示个人通讯组列表联系人地址的联系人卡片。</description>
            </item>
          </list>
          <para>
            <paramref name="fDelay" />参数仅适用于<paramref name="Style" />设置为"msoContactCardStyleHover"。对于所有其他卡样式，将忽略 fDelay。</para>
          <para />
        </remarks>
      </Docs>
    </Member>
